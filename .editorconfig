# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[{*.sh,*.md}]
indent_style = space
indent_size = 2
max_line_length = 180
trim_trailing_whitespace = true

[*.xml]
indent_style = space
indent_size = 4

[.idea/**.xml]
indent_size = 2
